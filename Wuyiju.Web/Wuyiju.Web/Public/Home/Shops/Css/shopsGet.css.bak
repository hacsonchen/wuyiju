.companyInfo{border-left:1px solid #e5e5e5; border-right:1px solid #e5e5e5;}
.companyInfo .hd{text-indent:30px; font-size:18px; height:50px; line-height:50px;}
.companyInfo-tbl{width:100%; border-collapse:collapse; border-bottom:1px solid #e5e5e5;}
