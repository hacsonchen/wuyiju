.complaintLabel{vertical-align:middle; display:inline-block;}
.complaintTxt{border:none; background:#fff; height:37px; color:#666; font:18px/37px 'Microsoft Yahei'; vertical-align:middle; width:145px;}
.complaintArea{resize:none; width:655px; height:105px; line-height:1.2; /*padding:8px 0;*/ margin-top:8px; margin-bottom:10px;}
.complaintContents{float:none; margin-right:0; vertical-align:top; padding:8px 0 8px 12px; width:768px;}
.dottedLine{border-bottom:2px dotted #e8e8e8; height:0px; background:#fff; overflow:hidden; margin:0 5px;}
.uploadE{background:#fff; padding:8px 6px;}
.uploadE .evidence{width:118px; height:118px; background:#e6e6e6; margin:0 5px; text-align:center; float:left; position:relative;}
.uploadBtn{display:inline-block; margin-top:45px; padding:0 5px; background:url(../Images/uploadevidenceicon.png) no-repeat; padding-left:26px; height:23px; font:14.74px/23px 'Microsoft Yahei'; color:#4e4e4e;}
.uploadBtn:hover{color:#4e4e4e;}
.complaintOKbtn{width:180px; height:49px; background:#3cacdf; color:#fff; font:26.34px/1 'Microsoft Yahei'; border:none;}

.complete{background:none; padding:0 5px; word-break:break-all;}
.flieUpLoad{width:100%; height:100%; position:absolute; top:0; left:0; cursor:pointer; opacity:0; filter:alpha(opacity=0);}

.main-in{width:800px; margin:0 auto;min-height:400px;}