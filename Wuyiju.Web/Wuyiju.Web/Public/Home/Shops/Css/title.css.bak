/* CSS Document */
/*============= 菜单 ===========*/
.menu{width:1190px; margin:0 auto;}
.menu ul{border-bottom:3px solid #ff4400;}
.menu ul li{float:left; padding:6px 13px;}
.menu ul li.cur{background:#ff4400;}
.menu ul li a{text-decoration:none; color:#000; font-size:15px; font-family:"微软雅黑";}
.menu ul li.cur a{color:#fff;}

/*=================================== 内容 =================================*/
.content{padding:20px; width:1150px; margin:0 auto; line-height:24px; }
.content .title{margin-bottom:10px; font-family:"微软雅黑"; color:#000;}
.content .title h2{font-size:25px; display:inline; font-weight:normal; vertical-align:bottom;}
.content .title span{font-size:15px; vertical-align:bottom; margin-left:5px;}

/*---------------- 关于我们 ---------------*/
.content .con-in{font-size:14px; color:#666; margin:0 25px; border-bottom:1px dotted #939393;}

.content .first{line-height:25px;}
.content .first img{vertical-align:bottom; margin-left:42px; margin-top:-20px; *margin-top:-10px; float:right; display:inline;}
.content .first p{margin-top:15px;}

.content .second{padding:40px 0;}
.content .second ul{margin-left:15px;}
.content .second ul li{line-height:32px;}

.content .third{padding:40px 0; border:none;}

.content .address{padding:10px 0 75px; text-align:center;}

/*---------------- 商务合作 ---------------*/
.content .swhz{border:none; margin-top:35px; padding-bottom:100px;}
.content .swhz .fl{width:490px;}
.content .swhz .fr img{margin-top:-40px;}

.content .swhz .blue-tip{color:#7188A9; font-size:14px; padding-left:25px; height:60px; line-height:60px;}

.content .swhz dl{font-size:12px; padding:4px 0;}
.content .swhz dl dt{width:100px; text-align:right; float:left; display:inline; height:32px; line-height:32px;}
.content .swhz dl dt i{color:#d80000; font-style:normal;}
.content .swhz dl dd{float:left; display:inline; padding-left:30px; height:32px; line-height:32px;}
.content .swhz dl dd input{vertical-align:middle; border:1px solid #d4d4d4; height:30px; line-height:30px\9; width:240px; padding-left:10px;}
.content .swhz dl dd .submit-btn{background:url(../Images/ok-btn.png); width:109px; height:30px; text-indent:-9999px; border:none; display:block;}
.content .swhz dl dd.tips-dd{padding-left:10px; width:105px; overflow:hidden;}

